<% include ../header %>
    <table style="width:100%">
        <tbody>
            <tr>
                <td>
                    <input class=input_name type="text" style="width:20%" class="form-control" placeholder="이름"/>
                    <input class=input_no type="text" style="width:20%" class="form-control" placeholder="수강생 번호"/>
                </td>
            </tr>
        </tbody>
    </table>
    <fieldset>
        <legend><br><%=modulenames[0].module_kr%> 필기 평가 문제 페이지 <br>이름과 수강생 번호를 반드시 입력하세요</legend>
    <% for (var i=0 ; i < questions.length; i++ ) { %>
        <h4><b><%=questions[i].question%></b><h4>
        <div class="combo" data-combo="<%=questions[i].q_no%>">
        <% for (var j=0 ; j < questions[i].choices.length; j++ ) { %>
            <input type="radio" name="q_<%=questions[i].q_no%>" data-mnum="<%=questions[i].m_nos[j]%>" onClick=""> <%=questions[i].choices[j]%><br>
        <% } %>
        </div>
    <% } %>
    <legend></legend>
        <button class="register" type="">제출</button>
    </fieldset>

<script src="/js/onlinetest_testpage.js"></script>
<% include ../footer %>